    "version": "0.2.0",
    "configurations": [
        {
            "name": "Local: Json file",
            "type": "python",
            "request": "launch",
            "program": "docker-controller-client.py",
            "console": "integratedTerminal",
            "args": ["-f", "./json_example/config.json", "-d"]
        },
        {
            "name": "Remote: To Local Server",
            "type": "python",
            "request": "launch",
            "program": "docker-controller-client.py",
            "console": "integratedTerminal",
            "args": ["-u", "http://localhost:8000", "-i", "my-device", "-d"]
        }
    ]
}